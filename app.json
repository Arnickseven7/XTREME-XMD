{
  "name": "Mr CHAGRIN",
  "description": "Un compagnon discret mais efficace pour ton WhatsApp, propulsé par Mr CHAGRIN",
  "logo": "https://cdn.pixabay.com/photo/2016/11/29/09/32/emoji-1869790_1280.png",
  "keywords": ["bot"],
  "success_url": "/",
  "stack": "container",
  "env": {
    "SESSION_ID": {
      "description": "Put the session-id here.",
      "required": true,
      "value": ""
    },  
    "ALIVE_IMG": {
      "description": "Image à afficher lorsque le bot est en ligne",
      "required": true,
      "value": "https://cdn.pixabay.com/photo/2016/11/29/09/32/emoji-1869790_1280.png"
    }, 
    "ALIVE_MSG": {
      "description": "Message du bot lorsqu'il est en ligne",
      "required": true,
      "value": "Bot en ligne ✅"
    },
    "PREFIX": {
      "description": "Préfixe des commandes",
      "required": true,
      "value": "."
    },
    "MODE": {
      "description": "Public/Privé/Groupe",
      "required": true,
      "value": "public"
    },
    "AUTO_VOICE": {
      "description": "Activer la réponse vocale automatique",
      "required": true,
      "value": "false"
    }, 
    "AUTO_REPLY": {
      "description": "Activer les réponses automatiques",
      "required": true,
      "value": "false"
    }, 
    "AUTO_STICKER": {
      "description": "Activer l'auto-sticker",
      "required": true,
      "value": "false"
    }, 
    "AUTO_READ_STATUS": {
      "description": "Lire automatiquement les statuts après 10 secondes",
      "required": true,
      "value": "true"
    }, 
    "ANTI_LINK": {
      "description": "Supprimer automatiquement les liens dans les groupes où tu es admin",
      "required": true,
      "value": "true"
    },
    "ANTI_BAD": {
      "description": "Supprimer les mots interdits dans les groupes où tu es admin",
      "required": true,
      "value": "true"
    }, 
    "FAKE_RECORDING": {
      "description": "Activer l'effet de faux enregistrement",
      "required": true,
      "value": "false"
    },
    "AUTO_REACT": {
      "description": "Réaction automatique aux messages",
      "required": false,
      "value": "false"
    },
    "OWNER_REACT": {
      "description": "Activer les réactions réservées au propriétaire",
      "required": false,
      "value": "true"
    }, 
    "BOT_NAME": {
      "description": "Nom du bot",
      "required": true,
      "value": "Mr CHAGRIN"
    }, 
    "HEART_REACT": {
      "description": "Réaction uniquement avec ♥️",
      "required": true,
      "value": "false"
    },
    "OWNER_NUMBER": {
      "description": "Nom du propriétaire",
      "required": true,
      "value": "Mr CHAGRIN"
    }
  },
  "buildpacks": [
    {
      "url": "https://github.com/heroku/heroku-buildpack-nodejs.git"
    }
  ],
  "stack": "heroku-24"
}
